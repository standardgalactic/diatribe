        np.roll(phi, </span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">1</span></span><span>) + np.roll(phi, -</span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">1</span></span><span>) -
        </span><span><span class="hljs-number">4</span></span><span> * phi
    )
    phi += D * dt * laplacian
    
